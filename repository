function drawRange(id)
{
    var coverage = {
    
    };

    var cov = coverage[id.split('_')[1]];
    var width=$('#'+id).innerWidth();
    var height=$('#'+id).innerHeight();
    var scale = width / cov['seqlength'];
    var html = '';
    var coverage = null;
    var col = "rgba(30,90,255,.8)";
    if(id.indexOf('modcov')==0)
        coverage = cov['modcov'];
    else
    {
        coverage = cov['pdbcov'];
        col = "rgba(30,150,90,.8)";
    }
    var title = ""

    if(coverage)
    {
        var from=0; 
        for(var i=0; i<coverage.length; i++)
        {
            if(title.length>0)
                title+="; ";
            title += coverage[i][0]+"-"+coverage[i][1];

            var w = scale*(coverage[i][0]-from);
            if(coverage[i][0]>1)
            {
                w = Math.floor(w);
                if(w>0)
                    html += '<div style="float:left; padding:0; margin:0; float:left; background-color:#FFF;height:'+height+'px; width:'+w+'px;"/>';
            }

            w = scale*(coverage[i][1]-coverage[i][0]);
            w = Math.floor(w);
            html+='<div style="float:left; padding:0; margin:0; float:left; background-color:'+col+';height:'+height+'px; width:'+w+'px;"/>';

            from = coverage[i][1]
        }
    }
    
    $('#'+id)
        .append(html)
        .attr('title', title);
}


