email=`cat $WORKDIR/Drugbank_email`
password=`cat $WORKDIR/Drugbank_password`


if  [ ! -e drug?links.csv ]
then
    echo -n "Downloading the data from DrugBank, please wait..."
    wget -O drug?links.csv.zip --user $email --password $password https://go.drugbank.com/releases/5-1-7/downloads/approved-drug-links
    unzip -qn drug?links.csv.zip
    echo done.
fi
