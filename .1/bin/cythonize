#!/data/mgltools/MakeInstallers/python2.7/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Cython==0.22','console_scripts','cythonize'
__requires__ = 'Cython==0.22'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('Cython==0.22', 'console_scripts', 'cythonize')()
    )
